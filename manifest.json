{
    "name": "myUTCS events",
    "version": "1.0",
    "description": "search for UTCS events",
    "permissions": ["storage", "activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon28.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon28.png"
    },
    "manifest_version": 2
  }